структура Стан
  значення словник
  слухачі список
кінець

дія Стан.виконати_отримання_елемента(ключ)
  я.значення[ключ]
кінець

дія Стан.виконати_присвоєння_елемента(ключ, значення)
  я.значення[ключ] = значення
  
  перебрати я.слухачі як слухач
    слухач(я)
  кінець
кінець

дія Стан.виконати_створення(значення словник або пусто,
                            слухачі список або пусто)
  якщо значення є пусто
    значення = ()
  кінець
  якщо слухачі є пусто
    слухачі = []
  кінець

  я.значення = значення
  я.слухачі = слухачі
кінець

дія Стан.змінити(ключ текст, значення щось)
  я[ключ] = значення
кінець

дія Стан.слухати(слухач Дія)
  я.слухачі.додати(слухач)
кінець

дати Стан
